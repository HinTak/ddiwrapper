DLLS                  = gdi32.dll
EXES                  =



### Common settings

CEXTRA                = -mno-cygwin -fPIC -fpack-struct -D_ANONYMOUS_UNION= -D_ANONYMOUS_STRUCT= -D__INTERLOCKED_DECLARED -g -Wall
CXXEXTRA              = -mno-cygwin
RCEXTRA               =
INCLUDE_PATH          = -I../include -I/usr/local/include/wine -I/usr/include/wine -I/usr/include/wine/windows
DLL_PATH              =
LIBRARY_PATH          =
LIBRARIES             =


### gdi32.dll sources and settings

gdi32_dll_MODULE   = gdi32.dll
gdi32_dll_C_SRCS   = gdi32.c util.c

gdi32_dll_CXX_SRCS =
gdi32_dll_RC_SRCS  =
gdi32_dll_LDFLAGS  = -mno-cygwin
gdi32_dll_DLL_PATH =
gdi32_dll_DLLS     = odbc32 \
			ole32 \
			oleaut32 \
			ntdll
gdi32_dll_LIBRARY_PATH=
gdi32_dll_LIBRARIES= uuid

gdi32_dll_OBJS     = $(gdi32_dll_C_SRCS:.c=.o) \
			$(gdi32_dll_CXX_SRCS:.cpp=.o) \
			$(gdi32_dll_RC_SRCS:.rc=.res)



### Global source lists

C_SRCS                = $(gdi32_dll_C_SRCS)
CXX_SRCS              = $(gdi32_dll_CXX_SRCS)
RC_SRCS               = $(gdi32_dll_RC_SRCS)


### Tools

CC = winegcc
CXX = wineg++
RC = wrc
WINEBUILD = winebuild


### Generic targets

all: $(SUBDIRS) $(DLLS:%=%.so) $(EXES:%=%.so)

### Build rules

.PHONY: all clean dummy

$(SUBDIRS): dummy
	@cd $@ && $(MAKE)

gdi32.o: gdi32.c ../common.h

# Implicit rules

.SUFFIXES: .cpp .rc .res
DEFINCL = $(INCLUDE_PATH) $(DEFINES) $(OPTIONS)

.c.o:
	$(CC) -c $(CFLAGS) $(CEXTRA) $(DEFINCL) -o $@ $<

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(CXXEXTRA) $(DEFINCL) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(CXXEXTRA) $(DEFINCL) -o $@ $<

.rc.res:
	$(RC) $(RCFLAGS) $(RCEXTRA) $(DEFINCL) -fo$@ $<

# Rules for cleaning

CLEAN_FILES     = *.dbg.c y.tab.c y.tab.h lex.yy.c \
                  core *.orig *.rej \
                  \\\#*\\\# *~ *% .\\\#*

clean:: $(SUBDIRS:%=%/__clean__) $(EXTRASUBDIRS:%=%/__clean__)
	$(RM) $(CLEAN_FILES) $(RC_SRCS:.rc=.res) $(C_SRCS:.c=.o) $(CXX_SRCS:.cpp=.o)
	$(RM) $(DLLS:%=%.dbg.o) $(DLLS:%=%.so)
	$(RM) $(EXES:%=%.dbg.o) $(EXES:%=%.so) $(EXES:%.exe=%)

$(SUBDIRS:%=%/__clean__): dummy
	cd `dirname $@` && $(MAKE) clean

$(EXTRASUBDIRS:%=%/__clean__): dummy
	-cd `dirname $@` && $(RM) $(CLEAN_FILES)

### Target specific build rules

$(gdi32_dll_MODULE).dbg.c: $(gdi32_dll_C_SRCS) $(gdi32_dll_CXX_SRCS)
	$(WINEBUILD) -o $@ --debug -C$(SRCDIR) $(gdi32_dll_C_SRCS) $(gdi32_dll_CXX_SRCS)

$(gdi32_dll_MODULE).so: $(gdi32_dll_MODULE).dbg.o $(gdi32_dll_OBJS)
	$(CC) -shared $(gdi32_dll_MODULE).spec $(gdi32_dll_LDFLAGS) -o $@ $(gdi32_dll_OBJS) $(gdi32_dll_MODULE).dbg.o $(gdi32_dll_LIBRARY_PATH) $(LIBRARY_PATH) $(gdi32_dll_DLLS:%=-l%) $(gdi32_dll_LIBRARIES:%=-l%)


